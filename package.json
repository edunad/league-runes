{
    "name": "rune",
    "packageManager": "yarn@3.2.1",
    "private": true,
    "version": "0.0.1",
    "scripts": {
        "start": "vite-node ./src/index.ts",
        "build": "tsc",

        "lint": "eslint ./src/**/*.{ts,tsx} --quiet",
        "pretty": "pretty-quick && yarn lint --fix",
        "prepare": "husky install"
    },
    "engines": {
        "npm": "Please use yarn",
        "node": ">=17.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.12",
        "fs-extra": "^10.1.0",
        "inquirer": "^9.0.0",
        "league-connect": "^6.0.0-rc3",
        "node-fetch": "^3.2.6",
        "terminal-art": "^1.4.3",
        "typescript": "^4.5.4",
        "vite": "^2.9.9"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.31",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "tslib": "^2.4.0",
        "vite-node": "^0.17.0"
    }
}
